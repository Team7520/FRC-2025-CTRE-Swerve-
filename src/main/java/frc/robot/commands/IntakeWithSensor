package frc.robot.commands;

import edu.wpi.first.wpilibj2.command.WaitCommand;
import frc.robot.Constants;
import frc.robot.subsystems.EndEffectorSubsystem;

public class IntakeWithSensor extends runCommand {
    public IntakeWithSensor(EndEffectorSubsystem endEffector, double conveyorSpeed) {
        addCommands(
            endEffector.setConveyorSpeedCommand(conveyorSpeed).until(() -> endEffector.StopWithSensor())
        );
    }
}